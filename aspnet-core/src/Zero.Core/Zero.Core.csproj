    <EmbeddedResource Include="Abp\Localization\Zero\Zero.xml" />
    <EmbeddedResource Include="Abp\Net\Emailing\EmailTemplates\default.html" />
    <EmbeddedResource Include="Abp\Localization\Zero\*.xml;Abp\Net\Emailing\EmailTemplates\default.html" Exclude="bin\**;obj\**;**\*.xproj;packages\**;@(EmbeddedResource)" />
  </ItemGroup>

  <ItemGroup>
