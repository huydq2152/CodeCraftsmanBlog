    <EmbeddedResource Include="Abp\Localization\Zero\Zero-th.xml" />
    <EmbeddedResource Include="Abp\Localization\Zero\Zero-tr.xml" />
    <EmbeddedResource Include="Abp\Localization\Zero\Zero-vi.xml" />
